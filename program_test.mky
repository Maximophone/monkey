let while = fn(n_iterations, action){
	if(n_iterations > 0){
		action();
		return while(n_iterations - 1, action);
	};
};

let print_test = fn(){
	puts("test");
};

while(5, print_test);

let factorial = fn(n){
	if(n < 1){
		return n;
	}
	if(n == 1){
		return 1;
	}
	else{
		return factorial(n-1) * n;
	}
};

factorial(10);

while(96, fn(){});

let digit_to_str = fn(digit){
	if(digit==0){
		return "0";
	};
	if(digit==1){
		return "1";
	};
	if(digit==2){
		return "2";
	};
	if(digit==3){
		return "3";
	}
	if(digit==4){
		return "4";
	}
	if(digit==5){
		return "5";
	}
	if(digit==6){
		return "6";
	}
	if(digit==7){
		return "7";
	}
	if(digit==8){
		return "8";
	}
	if(digit==9){
		return "9";
	}
};

let mod = fn(a, b){
	return a - (a/b)*b;
}

let int_to_str = fn(number){
	let r = mod(number, 10);
	let q = number/10;
	if(q==0){
		return digit_to_str(r);
	};
	return int_to_str(q) + digit_to_str(r);
}

digit_to_str(2);
int_to_str(108973498);